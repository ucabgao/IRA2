{
  "name": "CSV Viewer",
  "version": "0.1",
  "description": "Allows viewing csv files (.csv) directly inside Chrome.",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*", "file://*/*"
      ],
      "js": ["csvify.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
